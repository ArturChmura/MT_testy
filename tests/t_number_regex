program {
    double a;
    int b;
    a = 0.0;
    a = 0.1;
    a = 1.0;
    a = 11240.0;
    a = 124124.0012000;
    a = 12412400;
    a = 1220401;
    a = -0.0;
    a = -0.1;
    a = -1.0;
    a = -11240.0;
    a = -124124.0012000;
    a = -12412400;
    a = -1220401;
    b = 0;
    b = 1024;
    b = -12442;
    b = 100000;
    write a;
    write "\n";
    write b;
}